			<xs:element name="localID" type="eCH-0129:namedIdType" maxOccurs="unbounded"/>
			<xs:element name="otherID" type="eCH-0129:namedIdType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
