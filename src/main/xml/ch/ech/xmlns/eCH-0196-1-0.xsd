			<xs:documentation xml:lang="de">Typdefinition zum Bestand (Mutation) des Titels für das Wertschriftenverzeichnis.</xs:documentation>
			<xs:documentation xml:lang="fr"/>
		</xs:annotation>
		<xs:sequence>
			<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Beliebige Erweiterungen des Finanzinstituts.</xs:documentation>
					<xs:documentation xml:lang="fr">Extension libre de l'établissement financier.</xs:documentation>
				</xs:annotation>
			</xs:any>
		</xs:sequence>
		<xs:attribute name="referenceDate" type="xs:date" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Der Stichtag zum Bestand oder Mutation.</xs:documentation>
				<xs:documentation xml:lang="fr">Date critère du stock ou de la modification.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="mutation" type="xs:boolean" use="optional" default="0">
			<xs:annotation>
				<xs:documentation xml:lang="de">Das Mutations-Flag zum Bestand oder Mutation.</xs:documentation>
				<xs:documentation xml:lang="fr">Drapeau de modification du stock ou de la modification.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="name" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Die Bezeichnung zum Bestand oder Mutation.</xs:documentation>
				<xs:documentation xml:lang="fr">Désignation du stock ou de la modification.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="200"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="quotationType" type="eCH-0196:quotationTypeType" use="optional" default="PIECE">
			<xs:annotation>
				<xs:documentation xml:lang="de">Die Notationsart zur Anzahl (Stück oder Nominal).</xs:documentation>
				<xs:documentation xml:lang="fr">Type de cotation du nombre (pièce ou nominal).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="quantity" type="xs:decimal" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Die Anzahl (Stück oder Nominal) zum Bestand (+) oder Mutation (+/-) zum Stichtag.</xs:documentation>
				<xs:documentation xml:lang="fr">Nombre (pièce ou nominal) du stock (+) ou de la modification (+/-) à la date critère.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="balanceCurrency" type="eCH-0196:currencyIdISO3Type" use="optional" default="CHF">
			<xs:annotation>
				<xs:documentation xml:lang="de">Die Währung zum Steuerkurs (Stückpreis oder Prozentwert).</xs:documentation>
				<xs:documentation xml:lang="fr">Monnaie du cours fiscal (pièce ou pourcentage).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="unitPrice" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Der Steuerkurs (Stückpreis oder Prozentwert) zum Stichtag und Währung.</xs:documentation>
				<xs:documentation xml:lang="fr">Cours fiscal (pièce ou pourcentage) à la date critère et monnaie.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="balance" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Der Saldo zum Stichtag und Währung.</xs:documentation>
				<xs:documentation xml:lang="fr">Solde à la date critère et monnaie.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="reductionCost" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Die Gewinnungskosten beim Kauf- oder Verkauf. Die Gewinnungskosten sind nur bei reiner Differenzbesteuerung relevant.</xs:documentation>
				<xs:documentation xml:lang="fr">Frais d'acquisition à l'achat ou la vente. Les frais d'acquisition ne sont pertinents que pour l'imposition de la différence pure.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="exchangeRate" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Der Devisenkurs zum Stichtag und Währung.</xs:documentation>
				<xs:documentation xml:lang="fr">Cours de change à la date critère et monnaie.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="currency" type="eCH-0196:currencyIdISO3Type" use="optional" fixed="CHF">
			<xs:annotation>
				<xs:documentation xml:lang="de">Die Währung zum Steuerwert.</xs:documentation>
				<xs:documentation xml:lang="fr">Monnaie de la valeur fiscale.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="value" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Der Steuerwert zum Stichtag.</xs:documentation>
				<xs:documentation xml:lang="fr">Valeur fiscale à la date critère pour l'impôt fédéral ou cantonal.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="blocked" type="xs:boolean" use="optional" default="0">
			<xs:annotation>
				<xs:documentation xml:lang="de">Das Gesperrt-Flag zum Titel.</xs:documentation>
				<xs:documentation xml:lang="fr">Drapeau de blocage du titre.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="blockingTo" type="xs:date" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Das Gesperrt-Bis Datum zum Titel.</xs:documentation>
				<xs:documentation xml:lang="fr">Date de fin de blocage du titre.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:anyAttribute namespace="##other" processContents="lax">
			<xs:annotation>
				<xs:documentation xml:lang="de">Beliebige Erweiterungen des Finanzinstituts.</xs:documentation>
				<xs:documentation xml:lang="fr">Extension libre de l'établissement financier.</xs:documentation>
			</xs:annotation>
		</xs:anyAttribute>
	</xs:complexType>
	<xs:complexType name="securityTaxValueType">
