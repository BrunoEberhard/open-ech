<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:eCH-0007="http://www.ech.ch/xmlns/eCH-0007/6" xmlns:eCH-0008="http://www.ech.ch/xmlns/eCH-0008/3" xmlns:eCH-0010="http://www.ech.ch/xmlns/eCH-0010/6" xmlns:eCH-0097="http://www.ech.ch/xmlns/eCH-0097/2" xmlns:eCH-0196="http://www.ech.ch/xmlns/eCH-0196/1" targetNamespace="http://www.ech.ch/xmlns/eCH-0196/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xs:annotation>
		<xs:documentation xml:lang="de">SSK eSteuerauszug</xs:documentation>
		<xs:documentation xml:lang="fr"/>
	</xs:annotation>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0007/6" schemaLocation="http://www.ech.ch/xmlns/eCH-0007/6/eCH-0007-6-0.xsd"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0008/3" schemaLocation="http://www.ech.ch/xmlns/eCH-0008/3/eCH-0008-3-0.xsd"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0010/6" schemaLocation="http://www.ech.ch/xmlns/eCH-0010/6/eCH-0010-6-0.xsd"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0097/2" schemaLocation="http://www.ech.ch/xmlns/eCH-0097/2/eCH-0097-2-0.xsd"/>
	<xs:element name="taxStatement">
		<xs:annotation>
			<xs:documentation xml:lang="de">Basiselement des Steuerauszug.</xs:documentation>
			<xs:documentation xml:lang="fr"/>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="eCH-0196:taxStatementType">
					<xs:attribute name="minorVersion" type="xs:integer" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="de">Die Versionsnummer des Steuerauszugs.</xs:documentation>
							<xs:documentation xml:lang="fr"/>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="bankAccountNameType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die Kontobezeichnung.</xs:documentation>
			<xs:documentation xml:lang="fr"/>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="40"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="bankAccountNumberType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die Kontonummer.</xs:documentation>
			<xs:documentation xml:lang="fr"/>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="15"/>
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="clientNumberType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die Kundennummer.</xs:documentation>
			<xs:documentation xml:lang="fr"/>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="40"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="currencyIdISO3Type">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition des ISO3-Code zur Währung.</xs:documentation>
			<xs:documentation xml:lang="fr"/>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="depotNumberType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die Depotnummer.</xs:documentation>
			<xs:documentation xml:lang="fr"/>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="15"/>
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ibanType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition der IBAN (International Bank Account Number).</xs:documentation>
			<xs:documentation xml:lang="fr"/>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="15"/>
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="isinType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition der ISIN (International Securities Identifiication Number).</xs:documentation>
			<xs:documentation xml:lang="fr"/>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="12"/>
			<xs:maxLength value="12"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="leiType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition der LEI (Legal Entity Identifier) gemäss ROC (Regulatory Oversight Committee).</xs:documentation>
			<xs:documentation xml:lang="fr"/>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="20"/>
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="quotationTypeType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition der Notationsart (Stück oder Prozent).</xs:documentation>
			<xs:documentation xml:lang="fr"/>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="PIECE">
				<xs:annotation>
					<xs:documentation xml:lang="de">Stück</xs:documentation>
					<xs:documentation xml:lang="fr"/>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PERCENT">
				<xs:annotation>
					<xs:documentation xml:lang="de">Prozent</xs:documentation>
					<xs:documentation xml:lang="fr"/>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="securityCategoryType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition der Titelkategorie (Aktien, Anlagefonds, Anleihen, Derivative Instrumente, Devisen und Banknoten, Libor- und Swapsätze, Münzen und Edelmetalle, Optionen und Sonstige).</xs:documentation>
			<xs:documentation xml:lang="fr"/>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="BOND">
				<xs:annotation>
					<xs:documentation xml:lang="de">Anleihen</xs:documentation>
					<xs:documentation xml:lang="fr">Emprunts</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="COINBULL">
				<xs:annotation>
					<xs:documentation xml:lang="de">Münzen und Edelmetalle</xs:documentation>
					<xs:documentation xml:lang="fr">Monnaies et métaux précieux</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CURRNOTE">
				<xs:annotation>
					<xs:documentation xml:lang="de">Devisen und Banknoten</xs:documentation>
					<xs:documentation xml:lang="fr">Devises et billets de banque</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DEVT">
				<xs:annotation>
					<xs:documentation xml:lang="de">Derivative Instrumente</xs:documentation>
					<xs:documentation xml:lang="fr">Produits financiers dérivés</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FUND">
				<xs:annotation>
					<xs:documentation xml:lang="de">Anlagefonds</xs:documentation>
					<xs:documentation xml:lang="fr">Fonds de placement</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LIBOSWAP">
				<xs:annotation>
					<xs:documentation xml:lang="de">Libor- und Swapsätze</xs:documentation>
					<xs:documentation xml:lang="fr"/>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OPTION">
				<xs:annotation>
					<xs:documentation xml:lang="de">Optionen</xs:documentation>
					<xs:documentation xml:lang="fr">Options</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OTHER">
				<xs:annotation>
					<xs:documentation xml:lang="de">Andere</xs:documentation>
					<xs:documentation xml:lang="fr">autre</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SHARE">
				<xs:annotation>
					<xs:documentation xml:lang="de">Aktien</xs:documentation>
					<xs:documentation xml:lang="fr">Actions</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="securityNameType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition der Titelbezeichnung.</xs:documentation>
			<xs:documentation xml:lang="fr"/>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="60"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="securityTypeType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die Titelart zu den Titelkategorien.</xs:documentation>
			<xs:documentation xml:lang="fr"/>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="BOND.BOND">
				<xs:annotation>
					<xs:documentation xml:lang="de">Obligationen</xs:documentation>
					<xs:documentation xml:lang="fr">Obligations</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BOND.OPTION">
				<xs:annotation>
					<xs:documentation xml:lang="de">Optionsanleihen</xs:documentation>
					<xs:documentation xml:lang="fr">Emprunts à option</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BOND.CONVERTIBLE">
				<xs:annotation>
					<xs:documentation xml:lang="de">Wandelanleihen</xs:documentation>
					<xs:documentation xml:lang="fr">Emprunts convertibles</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="COINBULL.COINGOLD">
				<xs:annotation>
					<xs:documentation xml:lang="de">Gemünztes Gold</xs:documentation>
					<xs:documentation xml:lang="fr">Or monnayé</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="COINBULL.GOLD">
				<xs:annotation>
					<xs:documentation xml:lang="de">Gold</xs:documentation>
					<xs:documentation xml:lang="fr">Or</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="COINBULL.PALLADIUM">
				<xs:annotation>
					<xs:documentation xml:lang="de">Palladium</xs:documentation>
					<xs:documentation xml:lang="fr">Palladium</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="COINBULL.PLATINUM">
				<xs:annotation>
					<xs:documentation xml:lang="de">Platin</xs:documentation>
					<xs:documentation xml:lang="fr">Platine</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="COINBULL.SILVER">
				<xs:annotation>
					<xs:documentation xml:lang="de">Silber</xs:documentation>
					<xs:documentation xml:lang="fr">Argent</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CURRNOTE.CURRENCY">
				<xs:annotation>
					<xs:documentation xml:lang="de">Devisen</xs:documentation>
					<xs:documentation xml:lang="fr">Devises</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CURRNOTE.CURRYEAR">
				<xs:annotation>
					<xs:documentation xml:lang="de">Jahresendkurse Devisen</xs:documentation>
					<xs:documentation xml:lang="fr">Cours des devises à la fin de l'année</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DEVT.COMBINEDPRODUCT">
				<xs:annotation>
					<xs:documentation xml:lang="de">Kombinierte Produkte</xs:documentation>
					<xs:documentation xml:lang="fr">Produits combinés</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DEVT.FUNDSIMILARASSET">
				<xs:annotation>
					<xs:documentation xml:lang="de">Anlagefondsähnliche Vermögen</xs:documentation>
					<xs:documentation xml:lang="fr">Ensemble de biens assimilés à un fonds de placement</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DEVT.INDEXBASKET">
				<xs:annotation>
					<xs:documentation xml:lang="de">Index- und Basketzertifikate</xs:documentation>
					<xs:documentation xml:lang="fr">Certificats sur un indice ou un panier d'actions</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FUND.ACCUMULATION">
				<xs:annotation>
					<xs:documentation xml:lang="de">Thesaurierungsfonds</xs:documentation>
					<xs:documentation xml:lang="fr">Fonds de thésaurisation</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FUND.DISTRIBUTION">
				<xs:annotation>
					<xs:documentation xml:lang="de">Ausschüttungsfonds</xs:documentation>
					<xs:documentation xml:lang="fr">Fonds de distribution</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FUND.REALESTATE">
				<xs:annotation>
					<xs:documentation xml:lang="de">Immobilienfonds mit direktem Grundbesitz</xs:documentation>
					<xs:documentation xml:lang="fr">Fonds de placement avec immeubles en propriété directe</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LIBOSWAP.LIBOR">
				<xs:annotation>
					<xs:documentation xml:lang="de">Liborsätze</xs:documentation>
					<xs:documentation xml:lang="fr">Taux libor</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LIBOSWAP.SWAP">
				<xs:annotation>
					<xs:documentation xml:lang="de">Swapsätze</xs:documentation>
					<xs:documentation xml:lang="fr">Taux swap</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OPTION.CALL">
				<xs:annotation>
					<xs:documentation xml:lang="de">Call</xs:documentation>
					<xs:documentation xml:lang="fr">Call</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OPTION.PHANTOM">
				<xs:annotation>
					<xs:documentation xml:lang="de">Phantom</xs:documentation>
					<xs:documentation xml:lang="fr">Phantôme</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OPTION.PUT">
				<xs:annotation>
					<xs:documentation xml:lang="de">Put</xs:documentation>
					<xs:documentation xml:lang="fr">Put</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SHARE.BEARERCERT">
				<xs:annotation>
					<xs:documentation xml:lang="de">Inhaberzertifikat</xs:documentation>
					<xs:documentation xml:lang="fr">Certificat au porteur</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SHARE.BONUS">
				<xs:annotation>
					<xs:documentation xml:lang="de">Genussschein</xs:documentation>
					<xs:documentation xml:lang="fr">Bon de jouissance</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SHARE.COMMON">
				<xs:annotation>
					<xs:documentation xml:lang="de">Stammaktie</xs:documentation>
					<xs:documentation xml:lang="fr">Action ordinaire</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SHARE.COOP">
				<xs:annotation>
					<xs:documentation xml:lang="de">Anteilscheine Genossenschaft</xs:documentation>
					<xs:documentation xml:lang="fr">Parts sociales de société coopérative</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SHARE.LIMITED">
				<xs:annotation>
					<xs:documentation xml:lang="de">Stammanteile GmbH</xs:documentation>
					<xs:documentation xml:lang="fr">Parts sociales de Sàrl</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SHARE.NOMINAL">
				<xs:annotation>
					<xs:documentation xml:lang="de">Namesanktie</xs:documentation>
					<xs:documentation xml:lang="fr">Action nominative</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SHARE.PARTCERT">
				<xs:annotation>
					<xs:documentation xml:lang="de">Partizipationsschein</xs:documentation>
					<xs:documentation xml:lang="fr">Bon de participation</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SHARE.PREFERRED">
				<xs:annotation>
					<xs:documentation xml:lang="de">Vorzugsaktie</xs:documentation>
					<xs:documentation xml:lang="fr">Action préférentielle</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SHARE.TRANSFERABLE">
				<xs:annotation>
					<xs:documentation xml:lang="de">Inhaberaktie</xs:documentation>
					<xs:documentation xml:lang="fr">Action au porteur</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="valorNumberType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition der Valorennummer.</xs:documentation>
			<xs:documentation xml:lang="fr"/>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="100"/>
			<xs:maxInclusive value="999999999999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="signType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für eine Zeichenerklärung der Kursliste z.B. (I).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="3"/>
			<xs:maxLength value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tinType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition der TIN (Taxpayer Identification Number).</xs:documentation>
			<xs:documentation xml:lang="fr"/>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="9"/>
			<xs:maxLength value="11"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="taxStatementType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition des Steuerauszug.</xs:documentation>
			<xs:documentation xml:lang="fr"/>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="institution" type="eCH-0196:institutionType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Die Angaben zum Finanzinstitut.</xs:documentation>
					<xs:documentation xml:lang="fr">Informations sur l'établissement financier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="client" type="eCH-0196:clientType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Die Angaben zu dem oder den Kunden (Stichwort: Partnerstamm).</xs:documentation>
					<xs:documentation xml:lang="fr">Informations sur le ou les clients (mot-clé : fichier des partenaires).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="accompanyingLetter" type="eCH-0196:accompanyingLetterType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Das Begleitschreiben zum Steuerauszug.</xs:documentation>
					<xs:documentation xml:lang="fr">Lettre accompagnant le relevé fiscal.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="listOfBankAccounts" type="eCH-0196:listOfBankAccountsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Das Kontenverzeichnis des Steuerauszugs.</xs:documentation>
					<xs:documentation xml:lang="fr">Etat des comptes du relevé fiscal.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="listOfLiabilities" type="eCH-0196:listOfLiabilitiesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Das Schuldenverzeichnis des Steuerauszugs.</xs:documentation>
					<xs:documentation xml:lang="fr">Etat des dettes du relevé fiscal.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="listOfExpenses" type="eCH-0196:listOfExpensesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Das Spesenverzeichnis des Steuerauszugs.</xs:documentation>
					<xs:documentation xml:lang="fr">Etat des frais du relevé fiscal.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="listOfSecurities" type="eCH-0196:listOfSecuritiesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Das Wertschriftenverzeichnis des Steuerauszugs.</xs:documentation>
					<xs:documentation xml:lang="fr">Etat des titres du relevé fiscal.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="listOfLumpSumTaxCredit" type="eCH-0196:listOfLumpSumTaxCreditType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Das Verzeichnis zur pauschalen Steueranrechnung und Steuerrückbehalt USA des Steuerauszugs.</xs:documentation>
					<xs:documentation xml:lang="fr">Etat relatif à l'imputation forfaitaire d'impôt et à la retenue supplémentaire d'impôt USA du relevé fiscal.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Beliebige Erweiterungen des Finanzinstituts.</xs:documentation>
					<xs:documentation xml:lang="fr">Extension libre de l'établissement financier.</xs:documentation>
				</xs:annotation>
			</xs:any>
		</xs:sequence>
		<xs:attribute name="id" type="xs:ID" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Die eindeutige Id des Steuerauszugs. Dieses Attribut kann als Referenz (URI) für die Signatur verwendet werden.</xs:documentation>
				<xs:documentation xml:lang="fr">Identifiant du relevé fiscal. Cet attribut peut être utilisé comme référence (URI) pour la signature.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="creationDate" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Das Erstelldatum und Uhrzeit des Steuerauszugs.</xs:documentation>
				<xs:documentation xml:lang="fr">Date et heure de création du relevé fiscal.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="taxPeriod" type="xs:gYear" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Das Jahr der zugehörigen Steuerperiode.</xs:documentation>
				<xs:documentation xml:lang="fr">Année de la période fiscale concernée.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
