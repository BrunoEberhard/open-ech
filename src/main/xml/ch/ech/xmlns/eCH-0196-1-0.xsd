		<xs:attribute name="value" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Der Steuerwert zum Stichtag für die Staatssteuer.</xs:documentation>
				<xs:documentation xml:lang="fr">Valeur fiscale à la date critère pour l'impôt can-tonal.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="undefined" type="xs:boolean" use="optional" default="0">
			<xs:annotation>
				<xs:documentation xml:lang="de">Das Undefiniert-Flag für einen nicht verfügbaren Steuerwert.</xs:documentation>
				<xs:documentation xml:lang="fr">Drapeau de non définition pour une valeur fis-cale non disponible.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="kursliste" type="xs:boolean" use="optional" default="1">
			<xs:annotation>
				<xs:documentation xml:lang="de">Das Kursliste-Flag als Quelle für den Steuerwert.</xs:documentation>
				<xs:documentation xml:lang="fr">Drapeau de liste des cours en tant que source de la valeur fiscale.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:anyAttribute namespace="##other" processContents="lax">
			<xs:annotation>
				<xs:documentation xml:lang="de">Beliebige Erweiterungen des Finanzinstituts.</xs:documentation>
				<xs:documentation xml:lang="fr">Extension libre de l'établissement financier.</xs:documentation>
			</xs:annotation>
		</xs:anyAttribute>
	</xs:complexType>
	<xs:complexType name="lumpSumTaxCreditPaymentType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition zum Ertrag mit der pauschalen Steueranrechnung und Steuerrückbehalt USA.</xs:documentation>
			<xs:documentation xml:lang="fr"/>
		</xs:annotation>
		<xs:sequence>
			<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Beliebige Erweiterungen des Finanzinstituts.</xs:documentation>
					<xs:documentation xml:lang="fr">Extension libre de l'établissement financier.</xs:documentation>
				</xs:annotation>
			</xs:any>
		</xs:sequence>
		<xs:attribute name="paymentDate" type="xs:date" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Das Fälligkeitsdatum zum Bruttoertrag.</xs:documentation>
				<xs:documentation xml:lang="fr">Date d'échéance du revenu brut.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="exDate" type="xs:date" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Das Ex-Datum zum Bruttoertrag.</xs:documentation>
				<xs:documentation xml:lang="fr">Date de comptabilisation du revenu brut.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="name" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Die Bezeichnung zum Bruttoertrag.</xs:documentation>
				<xs:documentation xml:lang="fr">Désignation du revenu brut.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="200"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="quotationType" type="eCH-0196:quotationTypeType" use="optional" default="PIECE">
			<xs:annotation>
				<xs:documentation xml:lang="de">Die Notationsart zur Anzahl (Stück oder Nominal).</xs:documentation>
				<xs:documentation xml:lang="fr">Type de cotation du nombre (pièce ou nominal).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="quantity" type="xs:decimal" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Die Anzahl (Stück oder Nominal) zum Ex-Datum bzw. Fälligkeitsdatum.</xs:documentation>
				<xs:documentation xml:lang="fr">Nombre (pièce ou nominal) à la date de comp-tabilisation ou d'échéance.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="amountCurrency" type="eCH-0196:currencyIdISO3Type" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Die Währung zum Bruttobetrag (Stückpreis oder Prozentwert).</xs:documentation>
				<xs:documentation xml:lang="fr">Monnaie du montant brut (pièce ou pourcen-tage).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="amountPerUnit" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Der Bruttobetrag pro Stück oder Nominal zum Fälligkeitsdatum und Währung.</xs:documentation>
				<xs:documentation xml:lang="fr">Montant brut par pièce ou nominal à la date d'échéance et monnaie.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="amount" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Der Bruttobetrag zum Fälligkeitsdatum und Währung.</xs:documentation>
				<xs:documentation xml:lang="fr">Montant brut à la date d'échéance et monnaie.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="exchangeRate" type="xs:decimal" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Der Devisenkurs zum Fälligkeitsdatum und Währung.</xs:documentation>
				<xs:documentation xml:lang="fr">Cours de change à la date d'échéance et monnaie.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="currency" type="eCH-0196:currencyIdISO3Type" use="optional" fixed="CHF">
			<xs:annotation>
				<xs:documentation xml:lang="de">Die Währung des Bruttoertrag der Rubrik B.</xs:documentation>
				<xs:documentation xml:lang="fr">Monnaie du revenu brut de la rubrique B.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="grossRevenueB" type="xs:decimal" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Der Bruttoertrag der Rubrik B (ohne Quellensteueranspruch) für die Bundessteuer.</xs:documentation>
				<xs:documentation xml:lang="fr">Revenu brut de la rubrique B (sans droit au remboursement de l'impôt à la source) pour l'impôt fédéral.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="grossRevenueBCanton" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Der Bruttoertrag der Rubrik B (ohne Quellensteueranspruch) für die Staatssteuer. Der Wert ist nur anzugeben, wenn dieser vom Wert für die Bundessteuer abweicht.</xs:documentation>
				<xs:documentation xml:lang="fr">Revenu brut de la rubrique B (sans droit au remboursement de l'impôt à la source) pour l'impôt cantonal. Valeur à spécifier uniquement si elle diffère de la valeur pour l'impôt fédéral.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="lumpSumTaxCreditPercent" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Die Pauschale Steueranrechnung in Prozent (100% = 100).</xs:documentation>
				<xs:documentation xml:lang="fr">Imputation forfaitaire d'impôt en pourcentage (100 % = 100).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="lumpSumTaxCreditAmount" type="xs:decimal" use="optional" default="0">
			<xs:annotation>
				<xs:documentation xml:lang="de">Die Pauschale Steueranrechnung zum Bruttoertrag.</xs:documentation>
				<xs:documentation xml:lang="fr">Imputation forfaitaire d'impôt sur le revenu brut.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="nonRecoverableTaxPercent" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Der nicht rückforderbare Teil zur pauschalen Steueranrechnung in Prozent (100% = 100).</xs:documentation>
				<xs:documentation xml:lang="fr">Part non récupérable de l'imputation forfaitaire d'impôt en pourcentage (100 % = 100).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="nonRecoverableTaxAmount" type="xs:decimal" use="optional" default="0">
			<xs:annotation>
				<xs:documentation xml:lang="de">Der nicht rückforderbare Teil zur pauschalen Steueranrechnung.</xs:documentation>
				<xs:documentation xml:lang="fr">Part non récupérable de l'imputation forfaitaire d'impôt.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="additionalWithHoldingTaxUSA" type="xs:decimal" use="optional" default="0">
			<xs:annotation>
				<xs:documentation xml:lang="de">Der Steuerrückbehalt USA zum Bruttoertrag.</xs:documentation>
				<xs:documentation xml:lang="fr">Retenue supplémentaire d'impôt USA sur le re-venu brut.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="undefined" type="xs:boolean" use="optional" default="0">
			<xs:annotation>
				<xs:documentation xml:lang="de">Das Undefiniert-Flag für einen nicht verfügbaren Bruttoertrag.</xs:documentation>
				<xs:documentation xml:lang="fr">Drapeau de non définition pour un revenu brut non disponible.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="kursliste" type="xs:boolean" use="optional" default="1">
			<xs:annotation>
				<xs:documentation xml:lang="de">Das Kursliste-Flag als Quelle für den Bruttoertrag.</xs:documentation>
				<xs:documentation xml:lang="fr">Drapeau de liste des cours en tant que source pour le revenu brut.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="sign" type="eCH-0196:signType" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Die Zeichenerklärung zur Ausschüttung aus der Kursliste z.B. (I).</xs:documentation>
				<xs:documentation xml:lang="fr">Explication des signes relative à la distribution provenant de la liste des cours, p. ex. (I).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:anyAttribute namespace="##other" processContents="lax">
			<xs:annotation>
				<xs:documentation xml:lang="de">Beliebige Erweiterungen des Finanzinstituts.</xs:documentation>
				<xs:documentation xml:lang="fr">Extension libre de l'établissement financier.</xs:documentation>
			</xs:annotation>
		</xs:anyAttribute>
	</xs:complexType>
</xs:schema>
