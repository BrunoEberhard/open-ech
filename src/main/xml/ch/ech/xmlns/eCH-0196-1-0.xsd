		<xs:attribute name="kursliste" type="xs:boolean" use="optional" default="1">
			<xs:annotation>
				<xs:documentation xml:lang="de">Das Kursliste-Flag als Quelle für den Steuerwert.</xs:documentation>
				<xs:documentation xml:lang="fr">Drapeau de liste des cours en tant que source de la valeur fiscale.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:anyAttribute namespace="##other" processContents="lax">
