	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0101/1" schemaLocation="http://www.ech.ch/xmlns/eCH-0101/1/eCH-0101-1-0.xsd"/>
	
	<xs:complexType name="personSearchResultType">
		<xs:sequence>
			<xs:element name="personSearchResultEntry" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="eCH-0044:namedPersonIdType" />
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="openEchPersonExtensionType">
		<xs:sequence>
			<xs:element name="personExtendedInformation" type="eCH-0101:personExtendedInformationType" minOccurs="0"/>
			<xs:element name="contact" type="eCH-0046:contactType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	
